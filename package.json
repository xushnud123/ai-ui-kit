    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "typescript": "*"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ],
    "rules": {},
    "parserOptions": {
